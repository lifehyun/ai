package javaBasic.ch02.sec10;

public class DataType2 {

	public static void main(String[] args) {
		// 논리형 boolean 불린형 둘중에 하나인것 true,false만 가능
		boolean b1 = true;
		boolean b2 = false;
		
		boolean b3 = 10 < 8;
		System.out.println(b3);
		
		//== 같다라는 표시
		boolean b4 = 100 == 100; //같다 != 같지 않다
		System.out.println(b4);
		
	}

}
