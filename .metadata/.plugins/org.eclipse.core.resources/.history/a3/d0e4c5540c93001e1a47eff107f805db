package javaBasic.ch03.sec02;

public class Operator01 {

	public static void main(String[] args) {
		int number = 10;
		number++; //number = number + 1;					//넘버라는변수에 더하는것
		System.out.println(number);
	
		++number;
		
		number--; //number = number - 1;					//넘버라는변수에 빼는것
		--number;
		System.out.println(number);	
	}
		
					

}
